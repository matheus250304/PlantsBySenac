<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Lalezar&display=swap"
      rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Laila:wght@700&display=swap"
      rel="stylesheet" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous" />
    <link rel="stylesheet" href="http://localhost:3333/styles/style.css" />
    <title>PlantsBySenac</title>
  </head>
  <body>
    <header><%- include('./partials/header'); %></header>
    <main>
      <section>
        <%- include('./partials/main'); %>
        <!-- Button trigger modal -->
        <div class="centralizarBotao">
          <button
            type="button"
            class="btn btn-primary"
            data-toggle="modal"
            data-target="#jardimfrente">
            Jardim á frente
          </button>
          <!-- Modal -->
          <div
            class="modal fade"
            id="jardimfrente"
            tabindex="-1"
            role="dialog"
            aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Plantas</h5>
                  <button
                    type="button"
                    class="close"
                    data-dismiss="modal"
                    aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                  <% plants.forEach((plantas) => { %>
                  <ul>
                    <li>
                      <div>
                        <% if(plantas.categoria === 'teste'){ %>
                          <div class="cardplant card">
                            <img class="card-img-top" src="http://localhost:3333/images/logo.svg" alt="Card image cap">
                            <div class="card-body">
                              <h2 class="card-title"><%= plantas.nome %></h2>
                              <p class="card-text"><%= plantas.florada %></p>
                              <%= console.log(plantas.id) %>
                              <a href="http://localhost:3333/home/plantas/ <%= plantas.id %>" class="btn btn-primary">Saiba Mais</a>
                            </div>
                          </div>
                        <h2></h2>
                        <% } %>
                      </div>
                    </li>
                  </ul>
                  <% }) %>
                </div>
                <div class="modal-footer"></div>
              </div>
            </div>
          </div>
          <button
            type="button"
            class="btn btn-primary"
            data-toggle="modal"
            data-target="#jardimatras">
            Jardim de trás
          </button>
          <!-- Modal -->
          <div
            class="modal fade"
            id="jardimatras"
            tabindex="-1"
            role="dialog"
            aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Plantas</h5>
                  <button
                    type="button"
                    class="close"
                    data-dismiss="modal"
                    aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                  <% plants.forEach((plantas) => { %>
                    <ul>
                      <li>
                        <div>
                          <% if(plantas.categoria === 'atras'){ %>
                            <div class="cardplant card">
                              <img class="card-img-top" src="http://localhost:3333/images/logo.svg" alt="Card image cap">
                              <div class="card-body">
                                <h2 class="card-title"><%= plantas.nome %></h2>
                                <p class="card-text"><%= plantas.florada %></p>
                                <%= console.log(plantas.id) %>
                                <a href="http://localhost:3333/home/plantas/ <%= plantas.id %>" class="btn btn-primary">Saiba Mais</a>
                              </div>
                            </div>
                          <h2></h2>
                          <% } %>
                        </div>
                      </li>
                    </ul>
                    <% }) %>
                </div>
                <div class="modal-footer"></div>
              </div>
            </div>
          </div>
          <button
            type="button"
            class="btn btn-primary"
            data-toggle="modal"
            data-target="#jardimlateral">
            Jardim lateral
          </button>
          <!-- Modal -->
          <div
            class="modal fade"
            id="jardimlateral"
            tabindex="-1"
            role="dialog"
            aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Plantas</h5>
                  <button
                    type="button"
                    class="close"
                    data-dismiss="modal"
                    aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                  <% plants.forEach((plantas) => { %>
                    <ul>
                      <li>
                        <div>
                          <% if(plantas.categoria === 'lateral'){ %>
                            <div class="cardplant card">
                              <img class="card-img-top" src="http://localhost:3333/images/logo.svg" alt="Card image cap">
                              <div class="card-body">
                                <h2 class="card-title"><%= plantas.nome %></h2>
                                <p class="card-text"><%= plantas.florada %></p>
                                <%= console.log(plantas.id) %>
                                <a href="http://localhost:3333/home/plantas/ <%= plantas.id %>" class="btn btn-primary">Saiba Mais</a>
                              </div>
                            </div>
                          <h2></h2>
                          <% } %>
                        </div>
                      </li>
                    </ul>
                    <% }) %>
                </div>
                <div class="modal-footer"></div>
              </div>
            </div>
          </div>
        </div>
        <div id="caixaPlantas">
          <div class="plantinhas">
            <img src="http://localhost:3333/images/rosa.png" alt="rosa" />
          </div>
          <div class="plantinhas">
            <img src="http://localhost:3333/images/hortela.png" alt="hortela" />
          </div>
          <div class="plantinhas">
            <img
              src="http://localhost:3333/images/macaranga.png"
              alt="macaranga" />
          </div>
        </div>
      </section>
    </main>
    <footer id="Biotec"><%- include('./partials/footer'); %></footer>
    <script
      src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
      integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
      crossorigin="anonymous"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
      integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
      crossorigin="anonymous"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
      crossorigin="anonymous"></script>
  </body>
</html>
